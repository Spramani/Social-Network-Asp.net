<!DOCTYPE html>
<html>

<!-- Mirrored from html.crumina.net/html-olympus/01-LandingPage.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 23 Jan 2019 19:40:31 GMT -->
<head>

	<title>Landing Page</title>

	<!-- Required meta tags always come first -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="x-ua-compatible" content="ie=edge">

	<!-- Main Font -->
	<script src="~/Scripts/js/webfontloader.min.js"></script>

	<script>
		WebFont.load({
			google: {
				families: ['Roboto:300,400,500,700:latin']
			}
		});
	</script>

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/dist/css/bootstrap-reboot.css">
	<link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/dist/css/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/dist/css/bootstrap-grid.css">

	<!-- Main Styles CSS -->
	<link rel="stylesheet" type="text/css" href="~/Content/css/main.min.css">
	<link rel="stylesheet" type="text/css" href="~/Content/css/fonts.min.css">



</head>

<body class="landing-page">

	<div class="content-bg-wrap"></div>


	<!-- Header Standard Landing  -->

	<div class="header--standard header--standard-landing" id="header--standard">
		<div class="container">
			<div class="header--standard-wrap">

				<a href="#" class="logo">
					<div class="img-wrap">
						<img src="~/Content/img/logo.png" alt="Olympus">
						<img src="~/Content/img/logo-colored-small.png" alt="Olympus" class="logo-colored">
					</div>
					<div class="title-block">
						<h6 class="logo-title">Social Gram</h6>
						<div class="sub-title">SOCIAL NETWORK</div>
					</div>
				</a>

				<a href="#" class="open-responsive-menu js-open-responsive-menu">
					<svg class="olymp-menu-icon"><use xlink:href="~/Content/svg-icons/sprites/icons.svg#olymp-menu-icon"></use></svg>
				</a>

			</div>
		</div>
	</div>

	<!-- ... end Header Standard Landing  -->
	<div class="header-spacer--standard"></div>

	<div class="container">
		<div class="row display-flex">
			<div class="col col-xl-6 col-lg-6 col-md-12 col-sm-12 col-12">
				<div class="landing-content">
					<h1>Welcome to the Best Social Network in the World</h1>

					<a data-toggle="tab" href="#home" role="tab" class="btn btn-md btn-border c-white">Register Now!</a>
				</div>
			</div>

			<div class="col col-xl-5 col-lg-6 col-md-12 col-sm-12 col-12">

				<!-- Login-Registration Form  -->

				@RenderBody()


				<!-- ... end Login-Registration Form  -->
			</div>
		</div>
	</div>

	<script src="~/Vender/dashbord.js"></script>


	<!-- JS Scripts -->
	<script src="~/Scripts/js/jquery-3.2.1.js"></script>
	<script src="~/Scripts/js/jquery.appear.js"></script>
	<script src="~/Scripts/js/jquery.mousewheel.js"></script>
	<script src="~/Scripts/js/perfect-scrollbar.js"></script>
	<script src="~/Scripts/js/jquery.matchHeight.js"></script>
	<script src="~/Scripts/js/svgxuse.js"></script>
	<script src="~/Scripts/js/imagesloaded.pkgd.js"></script>
	<script src="~/Scripts/js/Headroom.js"></script>
	<script src="~/Scripts/js/velocity.js"></script>
	<script src="~/Scripts/js/ScrollMagic.js"></script>
	<script src="~/Scripts/js/jquery.waypoints.js"></script>
	<script src="~/Scripts/js/jquery.countTo.js"></script>
	<script src="~/Scripts/js/popper.min.js"></script>
	<script src="~/Scripts/js/material.min.js"></script>
	<script src="~/Scripts/js/bootstrap-select.js"></script>
	<script src="~/Scripts/js/smooth-scroll.js"></script>
	<script src="~/Scripts/js/selectize.js"></script>
	<script src="~/Scripts/js/swiper.jquery.js"></script>
	<script src="~/Scripts/js/moment.js"></script>
	<script src="~/Scripts/js/daterangepicker.js"></script>
	<script src="~/Scripts/js/simplecalendar.js"></script>
	<script src="~/Scripts/js/fullcalendar.js"></script>
	<script src="~/Scripts/js/isotope.pkgd.js"></script>
	<script src="~/Scripts/js/ajax-pagination.js"></script>
	<script src="~/Scripts/js/Chart.js"></script>
	<script src="~/Scripts/js/chartjs-plugin-deferred.js"></script>
	<script src="~/Scripts/js/circle-progress.js"></script>
	<script src="~/Scripts/js/loader.js"></script>
	<script src="~/Scripts/js/run-chart.js"></script>
	<script src="~/Scripts/js/jquery.magnific-popup.js"></script>
	<script src="~/Scripts/js/jquery.gifplayer.js"></script>
	<script src="~/Scripts/js/mediaelement-and-player.js"></script>
	<script src="~/Scripts/js/mediaelement-playlist-plugin.min.js"></script>

	<script src="~/Scripts/js/base-init.js"></script>
	<script defer src="~/fonts/fontawesome-all.js"></script>

	<script src="~/Content/Bootstrap/dist/js/bootstrap.bundle.js"></script>
	@RenderSection("Scripts", required: false)
</body>
</html>
