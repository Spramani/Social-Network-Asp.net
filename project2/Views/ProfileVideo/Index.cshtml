
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MenuLayoutPage.cshtml";
    Layout = "~/Views/Shared/_ProfileMenuLayoutPage1.cshtml";
    var video = ViewBag.ProfileIntrobgs;
}
}

<div class="container">

</div>

<div class="container">
    <div class="row">
        @foreach (var item in video)
        {
            <div class="col col-xl-3 col-lg-3 col-md-6 col-sm-12 col-12">

                <div class=" ui-block video-item">
                    <div class="video-frame col-4-width">


                        <div class="video-frame">
                            <video style="width:290px; height:200px;" controls="controls">
                                <source src="@item.media_path" type="video/mp4" />
                            </video>
                        </div>

                    </div>



                </div>

            </div>
        }
    </div>
</div>

